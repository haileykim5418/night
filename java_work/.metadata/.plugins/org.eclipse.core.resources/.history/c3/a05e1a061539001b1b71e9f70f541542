package operTest1208;

import java.util.Scanner;

public class Test1 {

	public static void main(String[] args) {
		int num = 0;
		int num2 = 0;
		int num3 = 0;
		int num4 = 0;
		
		num = 500;
		num2 = 100;
		num3 = 50;
		num4 = 10;
		
		Scanner s = new Scanner (System.in);
		System.out.print("지폐 : " );
		int put = s.nextInt();
		int a = put/num;
		
	
		System.out.println("500원의 갯수 : " + a);
		

	}

}
